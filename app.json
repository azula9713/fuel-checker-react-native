{
  "expo": {
    "name": "Fuel Checker",
    "slug": "fuel-checker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/fuel.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/fuel-finder.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/fuel.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.tsukiyomidev.fuelchecker",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
